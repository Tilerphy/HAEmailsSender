    <!--Default values-->
    <add key="DefaultIsHtmlBody" value="true"/>
    <add key="DefaultFrom" value="judywang@edb.com"/>
    <add key="DefaultFromName" value="Judy"/>


    <!--item options-->
    <!--map the field index-->
    <add key="To" value="Send to"/>

    <add key="CC" value="~DefaultCC"/>
